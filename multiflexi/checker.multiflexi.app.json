{
    "image": "",
    "name": "AbraFlexi Checker",
    "description": "Check AbraFlexi availbility",
    "executable": "\/usr\/bin\/abraflexi-client-config-checker",
    "setup": "",
    "cmdparams": "",
    "requirements": ["AbraFlexi"],
    "deploy": "apt install abraflexi-client-config",
    "homepage": "https://github.com/VitexSoftware/php-abraflexi-config",
    "topics": ["AbraFlexi", "FlexiBee", "Config", "Checker"],
    "uuid": "35478b99-c65f-4cdd-9ee1-744c55d9ac03",
    "multiflexi": "1.0.0",
    "environment": {
        "APP_DEBUG": {
            "type": "bool",
            "description": "Show debug messages",
            "defval": "False",
            "required": true
        },
        "RESULT_FILE": {
            "type": "string",
            "description": "write output json data to",
            "defval": "reminder_{ABRAFLEXI_COMPANY}.json",
            "required": false
        },
        "EASE_LOGGER": {
            "type": "string",
            "description": "write log messages using",
            "defval": "console|syslog",
            "required": false
        },
        "ABRAFLEXI_COMPANY": {
            "type": "string",
            "description": "",
            "defval": "demo_de"
        },
        "ABRAFLEXI_LOGIN": {
            "type": "string",
            "description": "AbraFlexi Login",
            "defval": "winstrom"
        },
        "ABRAFLEXI_PASSWORD": {
            "type": "string",
            "description": "AbraFlexi password",
            "defval": "winstrom"
        },
        "ABRAFLEXI_URL": {
            "type": "string",
            "description": "AbraFlexi Server URI",
            "defval": "https:\/\/demo.flexibee.eu:5434"
        }
        }
    
}
